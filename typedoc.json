{
  "$schema": "https://typedoc.org/schema.json",
  "out": "./docs/api",
  "entryPoints": [
    "./controllers/browserController.ts",
    "./controllers/internalController.ts",
    "./controllers/chatController.ts",
    "./controllers/cleanupController.ts",
    "./controllers/cronController.ts",
    "./controllers/errorController.ts",
    "./controllers/iaapaController.ts",
    "./controllers/jobController.ts",
    "./controllers/pageController.ts",
    "./types/common.ts",
    "./types/errors.ts",
    "./validators/schemas.ts",
    "./helpers/browserHelper.ts",
    "./di/container.ts"
  ],
  "plugin": ["typedoc-plugin-zod"],
  "tsconfig": "./tsconfig.json",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "includeVersion": true,
  "readme": "./docs/API_DOCUMENTATION.md",
  "name": "LocalBrowser Playwright API",
  "footer": "Generated automatically from TypeScript source code.",
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "lowercaseInternal": true
  },
  "markdown": {
    "headings": "github",
    "anchorPrefix": "doc-"
  }
}
